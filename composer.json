{
    "name": "ciloe/graphql-client-bundle",
    "description": "A symfony GraphQL Client Bundle based on GraphQL Client PHP",
    "type": "symfony-bundle",
    "keywords": ["GraphQL", "Client", "Bundle"],
    "autoload": {
        "psr-4": {
            "GraphQLClientBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "ciloe/graphql-client-php": "0.*",
        "symfony/config": "~3.4|~4.0",
        "symfony/http-kernel": "~3.4|~4.0",
        "symfony/http-foundation": "~3.4|~4.0",
        "symfony/dependency-injection": "~3.4|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy ESCRICH",
            "email": "escrichjimmy@gmail.com"
        }
    ]
}
