{
    "name": "ciloe/graph-client-bundle",
    "description": "A symfony bundle based on GraphQL Client PHP",
    "type": "library",
    "autoload": {
        "psr-4": {
            "GraphClientBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "ciloe/graph-client-php": "0.*",
        "symfony/config": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/http-foundation": "^4.1",
        "symfony/dependency-injection": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy ESCRICH",
            "email": "escrichjimmy@gmail.com"
        }
    ]
}
